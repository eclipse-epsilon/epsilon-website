rule Flowchart2Heading
	transform f : Source!Flowchart
	to h1 : Target!H1 {
	
	h1.value = f.name;
}

rule Action2Heading
	transform a : Source!Action
	to h1 : Target!H1 {
	
	h1.value = a.name;
}

rule Decision2Heading
	transform d : Source!Decision
	to h1 : Target!H1 {
	
	h1.value = d.name;
}

rule Transition2Heading
	transform t : Source!Transition
	to h1 : Target!H1 {
	
	h1.value = t.name;
}